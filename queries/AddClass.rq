# Adds new class %CLASS%. The URI of the TriplesMap is generated by %CLASS%_TM

PREFIX rr: <http://www.w3.org/ns/r2rml#>
PREFIX rml: <http://semweb.mmlab.be/ns/rml#>
INSERT DATA { 
    
    <%CLASS%_TM> a rr:TriplesMap;
    rml:logicalSource [  
        rml:source "XXXX";
        rml:referenceFormulation "XXXX"
    ];	   
    rr:subjectMap [
        rr:template "XXXX";
        rr:class <%CLASS%> 
    ] 
}