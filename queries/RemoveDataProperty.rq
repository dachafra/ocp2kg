PREFIX rr: <http://www.w3.org/ns/r2rml#>
DELETE { 
    ?triplesmap rr:predicateObjectMap ?pomnode.
    ?pomnode rr:predicate <http://data.europa.eu/a4g/ontology#exampledataprop>;
        	 rr:objectMap ?onode.
    ?onode rr:reference ?reference.
}
   WHERE {
        ?triplesmap rr:predicateObjectMap ?pomnode.
   		?pomnode rr:predicate <http://data.europa.eu/a4g/ontology#exampledataprop>;
        	 	rr:objectMap ?onode.
   		?onode rr:reference ?reference.
  		?triplesmap  rr:subjectMap ?subnode.
    	?subnode rr:class <http://data.europa.eu/a4g/ontology#exampleclass1>.
   }