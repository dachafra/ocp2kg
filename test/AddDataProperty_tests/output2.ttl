@prefix ns1: <http://www.w3.org/ns/r2rml#> .
@prefix ns2: <http://semweb.mmlab.be/ns/rml#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://myexampleuri.org/data#AirportMapping> a ns1:TriplesMap ;
    ns2:logicalSource [ ns2:referenceFormulation <http://semweb.mmlab.be/ns/ql#CSV> ;
            ns2:source "Airport.csv" ] ;
    ns1:predicateObjectMap [ ns1:objectMap [ ns2:reference "XXX" ;
                    ns1:datatype xsd:String ] ;
            ns1:predicate <http://myexampleuri.org/data#testproperty> ] ;
    ns1:subjectMap [ ns1:class <http://myexampleuri.org/data#Stop> ;
            ns1:template "http://airport.example.com/{id}" ] .

