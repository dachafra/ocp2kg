@prefix ns1: <http://semweb.mmlab.be/ns/rml#> .
@prefix ns2: <http://www.w3.org/ns/r2rml#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://myexampleuri.org/data#AirportChildMapping> a ns2:TriplesMap ;
    ns1:logicalSource [ ns1:referenceFormulation <http://semweb.mmlab.be/ns/ql#CSV> ;
            ns1:source "Airport.csv" ] ;
    ns2:predicateObjectMap [ ns2:objectMap [ ns1:reference "longitude" ] ;
            ns2:predicate <http://www.w3.org/2003/01/geo/wgs84_pos#long> ] ;
    ns2:subjectMap [ ns2:class <http://myexampleuri.org/data#SimpleStop>,
                <http://myexampleuri.org/data#Stop> ;
            ns2:template "http://airport.example.com/{id}" ] .

<http://myexampleuri.org/data#AirportMapping> a ns2:TriplesMap ;
    ns1:logicalSource [ ns1:referenceFormulation <http://semweb.mmlab.be/ns/ql#CSV> ;
            ns1:source "Airport.csv" ] ;
    ns2:predicateObjectMap [ ns2:objectMap [ ns1:reference "stop" ;
                    ns2:datatype xsd:int ] ;
            ns2:predicate <http://vocab.org/transit/terms/route> ],
        [ ns2:objectMap [ ns1:reference "latitude" ] ;
            ns2:predicate <http://www.w3.org/2003/01/geo/wgs84_pos#lat> ] ;
    ns2:subjectMap [ ns2:class <http://myexampleuri.org/data#Stop> ;
            ns2:template "http://airport.example.com/{id}" ] .

