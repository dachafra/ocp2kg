@prefix ex: <http://example.com/ns#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .

<http://example.com/ns##TransportMapping> a rr:TriplesMap ;
    rml:logicalSource [ rml:referenceFormulation ql:CSV ;
            rml:source "Transport.csv" ] ;
    rr:predicateObjectMap [ rr:objectMap [ rr:template "http://epo-changes.org/vehicle/{type}" ] ;
            rr:predicate ex:type ] ;
    rr:subjectMap [ rr:class ex:Transport ;
            rr:template "http://epo-changes.org/bus/{id}" ] .

<http://example.com/ns##exampleclass0> a rr:TriplesMap ;
    rml:logicalSource [ rml:referenceFormulation ql:CSV ;
            rml:source "exampleinputsource.csv" ] ;
    rr:predicateObjectMap [ rr:objectMap [ rr:template "http://epo-changes.org/{column}" ] ;
            rr:predicate ex:exampleprop ] ;
    rr:subjectMap [ rr:class ex:exampleclass0 ;
            rr:template "http://epo-changes.org/{id}" ] .

