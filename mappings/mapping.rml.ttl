<AddedClasses_0> a rr:TriplesMap;

	rml:logicalSource [
		a rml:LogicalSource;
		rml:source "added_classes.csv";
		rml:referenceFormulation ql:CSV
	];
	rr:subjectMap [
		a rr:SubjectMap;
		rr:template "http://epo-changes.org/{reference_version}-{target_version}/AddClass/{name}";
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant rdf:type;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:constant omv:AddClass;
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant omv:addedClass;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "name";
			rr:termType rr:IRI
		];
	].


